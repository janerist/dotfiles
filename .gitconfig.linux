[user]
        name = janerist
        email = janerist@fastmail.com
[core]
        editor = vim
[alias]
        l = log
        s = status
        b = branch
        ec = config --global -e
        co = checkout
        cob = checkout -b
        up = !git pull --rebase --prune $@ && git submodule update --init --recursive
        save = !git add -A && git commit -m 'SAVEPOINT'
        undo = reset HEAD~1 --mixed
